 
#############################################################################
# The default settings are defaults, they are used if they are NOT
# overridden by using an explicit section (as in "llxt w") or by the
# command line (as in "llxt --verbose 1")
#############################################################################

[default] 

# This is what you see when you list the sections (as in "llxt --list") 
desc: rxvt with dir and display ON 

# User and host name 
#user: bob 
host: localhost 

# Shell type to use, 'B'ourne = (sh, bash, ksh, zsh) 'C'shell = (csh, tcsh) 
shell: bourne 

# Communication type, supports ssh, rsh, and ftp to move .llxt_setup file to 
# the remotehost 
comm: ssh 

# Verbose = debugging level.  Supports 1 through 9 
optionVerbose: 0 

# Turn this ON if you want to set the $DISPLAY and xauth info on the remotehost 
optionDisplay: 1 

# Turn this ON if you want to automatically set the directory of the 
# remote host to the current one of the localhost, very handy 
optionDir: 1 

# Standard terminal (path may be required for SSH) and options you like 
# Note that $LLXT_REMOTEHOST will be translated to the targethost 
#           $LLXT_REMOTEUSER is username, others may follow 
#term: /usr/bin/rxvt  
term: con
termOptions: -fg gray50 -bg black -cr white -title $LLXT_REMOTEHOST -ls +sb -fn lucidasanstypewriter-4 -sl 1024

# Additional options for connecting to remotehosts, (not used in SSH) 
# these are appended to the end of the TERM setting to make the executable 
termRemoteOptions: -e rlogin -l $LLXT_REMOTEUSER $LLXT_REMOTEHOST 

# Turn this OFF if ICMP on your network is disabled (read: ping fails)
#optionPing: 0

#############################################################################
# These properties override what was defined in the [default] section.  Each
# section can be called directly as the first command line arg.  As in:
#    llxt svr
# Section properties can be appended to or overridden by the command line:
#    llxt er --user alice
#    llxt svr --host otherserver
#############################################################################

