clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add    Shift   = Shift_L Shift_R
add    Control = Control_L Control_R
add    Mod1    = Alt_L Alt_R
add    Mod2    = Num_Lock
add    Mod5    = Multi_key

! make caps a control
remove Lock = Caps_Lock
keysym Caps_Lock = Control_L
add Lock = Caps_Lock