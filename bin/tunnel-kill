#!/bin/sh

#export USRHOST="hgoodman@208.72.159.219" 
export USRHOST="heow@lispnyc.org"

for i in `ps -ef | grep ssh | grep ${USRHOST} | grep -v restart | grep "ssh -C -N " | awk '{ print $2 }'`; do
  echo "killing pid: $i"
  kill $i
done

exit

